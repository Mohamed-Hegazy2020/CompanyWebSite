    </EmbeddedResource>
    <EmbeddedResource Update="Resources\ProductsDataResources.en.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\ServicesDataResource.ar.resx">
