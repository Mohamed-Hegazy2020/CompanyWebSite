    <None Include="wwwroot\bootstrap-4.3.1-rtl\.github\ISSUE_TEMPLATE\feature.md" />
    <None Include="wwwroot\bootstrap-4.3.1-rtl\.github\ISSUE_TEMPLATE\feature_request.md" />
    <None Include="wwwroot\bootstrap-4.3.1-rtl\.github\SUPPORT.md" />
    <None Include="wwwroot\inner-page.html" />
