    </EmbeddedResource>
    <EmbeddedResource Update="Resources\CompanyDataResource.en.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\ProductsDataResources.ar.resx">
